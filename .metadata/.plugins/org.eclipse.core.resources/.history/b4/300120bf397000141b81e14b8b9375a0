public class Constant extends Term {

	public Constant() {
		super();
		edit.setText("1");
		weight = 1;
	}

	public Constant(int x, int y) {
		super(x, y);
		edit.setText("1");
		weight = 1;
	}

	public Constant cloneMe(int x, int y) {
		Constant x1 = new Constant(x, y);
		x1.weight = weight;
		return x1;
	}
}